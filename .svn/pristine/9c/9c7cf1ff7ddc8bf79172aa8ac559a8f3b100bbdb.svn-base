package factorial;

import java.math.BigInteger;

public class FactorialImperative implements Factorial {

  public BigInteger calculate(long number) { //Feedback: imperativeFactorial and move to one class
    //Feedback: no need for this blank line
    BigInteger product = BigInteger.ONE;

    for (int i = 1; i <= number; i++) {
      product = product.multiply(new BigInteger(String.valueOf(i)));
    }

    return product;
    //Feedback: no need for this blank line
  }
  //Feedback: no need for this blank line
}
