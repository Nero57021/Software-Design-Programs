package processor;

import org.junit.jupiter.params.provider.Arguments;
//Feedback: no need for a blank line here
import java.util.stream.Stream;

public class LowercaseZBlockerBlockTest implements BlockProcessorTest {
  private static Stream<Arguments> testSamples() {
    return Stream.of(
      Arguments.of("z", ""),
      Arguments.of("az", "a"),
      Arguments.of("1", "1"),
      Arguments.of("~", "~"),
      Arguments.of("", "")
    );
  }

  @Override
  public String processor(String input) {return BlockProcessor.createBlocker(input).apply("z");}
}
