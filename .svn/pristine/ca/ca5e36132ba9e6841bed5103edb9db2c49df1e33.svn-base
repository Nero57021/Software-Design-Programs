package factorial;

public class ImperativeFactorial implements Factorial { //Feedback: Factorial instead of ImperativeFactorial

    @Override
    public long computeFactorial(long num) { //Feedback: factorialImperative . number insted of num

        if(num < 0){
            throw new RuntimeException("Cannot be negative number");
        } //Feedback: a blank line after this line, please
        long res = 1; //Feedback: product
        for ( int i = 1; i <= num; i++ ) {
            res *= i;
        } //Feedback: a blank line after this line, please
        return res;
    }

    
}